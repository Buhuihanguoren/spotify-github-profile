{
  "version": 2,
  "builds": [
    {
      "src": "api/view.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    },
    {
      "src": "api/callback.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    },
    {
      "src": "api/login.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/login",
      "dest": "api/login.py"
    },
    {
      "src": "/api/callback",
      "dest": "api/callback.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/view.py"
    }
  ]
}